@import "./styles/iconfont.wxss";

/* 小程序中不支持通配符 */


/* 解决办法，把所有标签名写上 */

page,
view,
text,
swiper,
swiper-item,
image,
navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}


/* 主题颜色 */

page {
    /* 定义主题颜色 */
    --themColor: #ff5100;
    /* 定义统一的字体大小 */
    font-size: 28rpx;
}